<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/custom_button" android:state_pressed="false" />

    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#201D1D" />
            <corners android:radius="30dp" />
            <stroke android:width="7dp" android:color="#25541B" />
            <gradient android:endColor="#64BB69" android:startColor="@color/cardview_light_background" />
        </shape>
    </item>
</selector>